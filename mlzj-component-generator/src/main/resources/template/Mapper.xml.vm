<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="${package}.mapper.${className}PoMapper">
	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="${package}.entity.po.${className}Po" id="${classname}Map">

#foreach($column in $columns)
        <result column="${column.columnName}" jdbcType="${column.dataType}" property="${column.attrname}" />
#end
    </resultMap>

    <sql id="Base_Column_List">
#foreach($column in $columns)
        ${column.columnName},
#end
    </sql>

    <select id="query${className}ListByPage" resultType="${package}.entity.po.${className}Po">
        select
        <include refid="Base_Column_List" />
        from
        ${tableName}
        where del_flag = 0
        <if test="dto.state != null and dto.state != '' ">
            AND state = #{dto.state,jdbcType=VARCHAR}
        </if>
    </select>
</mapper>